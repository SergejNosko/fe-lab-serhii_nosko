{{#each data}}
    <li class="task-article__list-item single-task single-task_high single-task_sport">
        <div class="single-task__time">Today</div>
        <div class="single-task__description">
            <h3 class="single-task__title">{{title}}</h3>
            <p class="single-task__text">{{description}}</p>
        </div>
        <a href="#" class="single-task__edit-link icon-edit" title="Edit task"></a>
        <a href="timer.html" class="single-task__status icon-tomato" title="Go to Timer"></a>
    </li>
{{/each}}